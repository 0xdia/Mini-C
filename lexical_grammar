INT: int

FLOAT: float

WHILE: while

IF: if

ELSE: else

IDENTIFIER: [a-zA-Z_][a-zA-Z0-9_]*

NUMBER: [0-9]*[.][0-9]+ | [0-9]+

EQUAL: ==

GREATER: >

LOWER: <

GREATEQ: >=

LOWEQ: <=

NOTEQ: !=

COMMA: ,

SEMIC: ;

OPCUR: {

CLCUR: }

OPPAR: (

CLPAR: )

PLUS: +

MINUS: -

MULT: *

DIV: /

SKIP: " " | "\n" | "\r" | "\r\n"
